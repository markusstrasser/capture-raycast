{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "capture",
  "title": "CaptureBaby",
  "description": "Capture context and items from anywhere",
  "icon": "extension-icon.png",
  "author": "markusstrasser",
  "license": "MIT",
  "commands": [
    {
      "name": "capture",
      "title": "Quick Capture",
      "description": "Silently capture current context",
      "mode": "no-view",
      "keywords": [
        "snap",
        "screenshot",
        "save"
      ]
    },
    {
      "name": "comment-capture",
      "title": "Comment Captures",
      "description": "Add comments to your captures",
      "mode": "view"
    },
    {
      "name": "comment-screenshot",
      "title": "Comment Screenshots",
      "description": "Add comments to recent Screenshots",
      "mode": "view",
      "keywords": [
        "cleanshot",
        "screenshot",
        "comment"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.88.4",
    "@raycast/utils": "^1.17.0",
    "node-geocoder": "^4.4.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/node-geocoder": "^4.2.6",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
